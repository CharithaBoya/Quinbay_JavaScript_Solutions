let count = 0;
let res;
function cc(card) {
  // Only change code below this line
  if(card>=2 && card<=6){
    count += 1;
  }
  else if(card>=7 && card<=9){
    count += 0;
  }
  else{
    count -= 1;
  }

 if(count>0){
   res = "Bet";
 }
 else{
   res = "Hold";
 }
  return count + " " + res;
}

cc(2); cc(3); cc(7); cc('K'); cc('A');
