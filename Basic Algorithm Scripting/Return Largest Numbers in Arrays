function largestOfFour(arr) {
  let max,i,j;
  let l = [];
  for(i=0;i<4;i++){
    max = arr[i][0];
    for(j=1;j<4;j++){
      if(arr[i][j]>max)
        max = arr[i][j];
    }
   l.push(max);
  }
  return l;
}

largestOfFour([[4, 5, 1, 3], [13, 27, 18, 26], [32, 35, 37, 39], [1000, 1001, 857, 1]]);
